<% include ./layout/head %>
<% include ./layout/header %>

<link rel="stylesheet" type="text/css" href="/css/wallet.css">
<link rel="stylesheet" type="text/css" href="/css/m_wallet.css">

<div ap-widget="LoginRequired"></div>
<div id="transactionDetails" class="wallet_wrap">
    <div class="container">
        <div class="page_link">
            <ul> 
                <li><a href="/wallet" ap-translate="TRANSACTION.DEPOSIT">Deposit, withdrawal</a></li>
                <li class="active"><a href="/transactionDetails" ap-translate="TRANSACTION.ORDERS">Orders</a></li>
            </ul>
        </div>
        <div class="tab_container">
            <div class="tab_list">
                <ul>
                    <li class="active" data-tab="tab1" ap-translate="TRANSACTION.ORDERSHISTORY">Filled Orders</li>
                    <li data-tab="tab2" ap-translate="TRANSACTION.OUTSTANDING">Open Orders</li>
                    <li data-tab="tab3" ap-translate="TRANSACTION.TRANSHISTORY">Transfer History</li>
                </ul>
            </div>
            <div class="tab_contant">
                <div id="tab1" class="tab_cont">
                    <div class="" ap-widget='Trades' hide-header="true"></div>
                </div>
                <div id="tab2" class="tab_cont">
                    <div ap-widget='OpenOrders' hide-header="true"></div>
                </div>
                <div id="tab3" class="tab_cont">
                    <div ap-widget='AccountTransactions' hide-header="true"></div>
                </div>
            </div>
        </div>
    </div>
</div>



